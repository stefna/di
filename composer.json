{
	"name": "stefna/di",
	"description": "Lightweight dependency injection container",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Sundqvist",
			"email": "andreas@stefna.is"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "^8.2",
		"psr/container": "^2.0"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.9",
		"stefna/codestyle": "^1.12",
		"phpstan/phpstan": "^1.8",
		"bnf/phpstan-psr-container": "^1.0",
		"phpunit/phpunit": "^10.0",
		"phpstan/phpstan-phpunit": "^1.1",
		"tomasvotruba/type-coverage": "^0.0.7",
		"phpstan/extension-installer": "^1.2"
	},
	"autoload": {
		"psr-4": {
			"Stefna\\DependencyInjection\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Stefna\\DependencyInjection\\Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
